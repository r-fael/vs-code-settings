{
  "workbench.startupEditor": "newUntitledFile",
  "editor.fontSize": 14,
  "editor.fontFamily": "JetBrains Mono",
  "editor.fontLigatures": true,
  "editor.lineHeight": 1.8,
  "workbench.editor.labelFormat": "short",
  "terminal.integrated.fontFamily": "JetBrains Mono",
  "terminal.integrated.fontSize": 14,
  "editor.rulers": [80, 120],
  "explorer.confirmDelete": false,
  "workbench.colorTheme": "No Happiness in Colors Theme",
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "explorer.compactFolders": true,
  "editor.formatOnSave": true,
  "explorer.confirmDragAndDrop": false,
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "security.workspace.trust.untrustedFiles": "open",
  "[python]": {
    "editor.formatOnType": true
  },
  "window.commandCenter": false,
  "workbench.layoutControl.enabled": false,
  "workbench.activityBar.location": "hidden",
  "workbench.statusBar.visible": false,
  "symbols.hidesExplorerArrows": false,
  "workbench.iconTheme": "symbols",
  "editor.tabSize": 2,
  "workbench.editor.empty.hint": "hidden",
  "apc.activityBar": {
    "position": "bottom",
    "hideSettings": true,
    "size": 48,
    "itemMargin": 8,
    "itemSize": 32
  },
  "breadcrumbs.enabled": false,
  "editor.minimap.autohide": true,
  "editor.renderLineHighlight": "gutter",
  "window.titleBarStyle": "native",
  "apc.font.family": "JetBrains Mono",
  "editor.fontWeight": "bold",
  "apc.stylesheet": {
    ".xterm-rows": "background-image: url('')",
    ".margin::after": "z-index: 1; content: ''; top: -13rem; left: 86.7rem; width: 100%;position: absolute; transform: scale(0.4); width: 20rem; height: 41.5rem; background: url('https://static.wikia.nocookie.net/jjba/images/e/e5/The_world_c2532.png/revision/latest/scale-to-width/360?cb=20170423132439&path-prefix=pt-br')",
    ".title-label": "padding: 50px",
    ".title-label > h2": "display: none",
    ".title-label::after": "content: 'ラファエル'; position: absolute; width: 100px; height: 100px; color: orange; left: 6px; top: -2px; font-weight: bold",
    ".monaco-editor .view-ruler": "display: none",
    ".editor-actions": "display: none",
    ".nosidebar .inline-tabs-placeholder": "width: 75px",
    ".pane-header": "padding: 0 8px",
    ".pane-body": "padding: 8px",
    ".monaco-scrollable-element > .visible": "display: none",
    ".split-view-view:first-child .pane-header": "display: none !important",
    ".monaco-list-row": "border-radius: 4px",
    ".monaco-workbench .monaco-list:not(.element-focused):focus:before": "display: none;",
    ".monaco-highlighted-label": "font-weight: bold",
    ".monaco-editor .monaco-editor-background::after": "background",
  },
  "editor.scrollbar.vertical": "hidden",
  "editor.scrollbar.horizontal": "hidden",
  "editor.renderWhitespace": "all",
  "terminal.integrated.tabs.location": "left",
  "window.zoomLevel": -1,
}
